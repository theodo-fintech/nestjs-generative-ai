{
  "name": "nestjs-generative-ai",
  "version": "1.0.4",
  "description": "A selection of useful one-liner decorators, pipes and services to boost your NestJS APIs with Generative AI sweetness",
  "author": "Woody Rousseau <woodyr@sipios.com>",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "nest",
    "nestjs",
    "langchain",
    "langchainjs",
    "ai",
    "openai",
    "llm",
    "nodejs",
    "node",
    "typescript",
    "javascript"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "build:watch": "tsc -w",
    "release": "semantic-release",
    "lint": "eslint \"src/**/!(*.d).ts\" --fix",
    "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write"
  },
  "dependencies": {
    "@langchain/core": "^0.1.43",
    "@langchain/openai": "^0.0.18",
    "@nestjs/common": "^10.3.3",
    "class-validator": "^0.14.1",
    "langchain": "^0.3.5",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/testing": "10.3.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/changelog": "^6.0.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "28.1.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "codecov": "^3.8.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^28.1.3",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "tsconfig-paths": "^3.10.1",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=21",
    "pnpm": ">=8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "homepage": "https://github.com/sipios/nestjs-generative-ai#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sipios/nestjs-generative-ai.git"
  },
  "bugs": {
    "url": "https://github.com/liaoliaots/nestjs-generative-ai/issues"
  }
}
